# (C) Wayne Wang	
# Configuration for ngx_http_guard_module.


GUARD_LIB_MODULES="                               \
                     ngx_guard_snmpget            \
                  "

GUARD_HTTP_MODULES="                              \
                     ngx_http_guard_module        \
                   "

GUARD_LIB_SRCS="                                        \
                     $ngx_addon_dir/snmpget/snmpget.h   \
               "
GUARD_HTTP_SRCS="                                             \
                     $ngx_addon_dir/ngx_http_guard_module.c   \
                "


ngx_addon_name="ngx_http_guard_module"



HTTP_MODULES="$HTTP_MODULES \
		ngx_http_guard_module"

NGX_ADDON_SRCS="$NGX_ADDON_SRCS \
		$ngx_addon_dir/ngx_http_guard_module.c"

RTMP_DEPS="                                     \
             $ngx_addon_dir/snmpget/snmpget.h   \
          "




if [ -f auto/module ] ; then
     
      ngx_module_incs=$ngx_addon_dir
      ngx_module_deps=$RTMP_DEPS
      
      if [ $ngx_module_link = DYNAMIC ] ; then
         ngx_module_name="$GUARD_LIB_MODULES $GUARD_HTTP_MODULES"
         ngx_module_srcs="$GUARD_LIB_SRCS $GUARD_HTTP_SRCS"
      
         . auto/module
